{
    "compilerOptions": {
        "lib": [
          "es2019"
        ],
        "alwaysStrict": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "strictNullChecks": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules",
        "dist",
        "temp"
    ]
}