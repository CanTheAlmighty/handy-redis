{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-console": [
            false
        ],
        "variable-name": [
            "allow-leading-underscore"
        ],
        "arrow-parens": false,
        "max-line-length": [
            180
        ],
        "interface-name": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "member-access": [
            false
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "no-unused-variable": [
            true
        ],
        "no-inferrable-types": [
            true
        ],
        "trailing-comma": [
            true
        ]
    }
}